## scale - step/51_100
execute if score @s rga.config.scale.step matches 51..55 run scale set pehkui:step_height 0.55
execute if score @s rga.config.scale.step matches 56..60 run scale set pehkui:step_height 0.60
execute if score @s rga.config.scale.step matches 61..65 run scale set pehkui:step_height 0.65
execute if score @s rga.config.scale.step matches 66..70 run scale set pehkui:step_height 0.70
execute if score @s rga.config.scale.step matches 71..75 run scale set pehkui:step_height 0.75
execute if score @s rga.config.scale.step matches 76..80 run scale set pehkui:step_height 0.80
execute if score @s rga.config.scale.step matches 81..85 run scale set pehkui:step_height 0.85
execute if score @s rga.config.scale.step matches 86..90 run scale set pehkui:step_height 0.90
execute if score @s rga.config.scale.step matches 91..95 run scale set pehkui:step_height 0.95
execute if score @s rga.config.scale.step matches 96..100 run scale set pehkui:step_height 1.00